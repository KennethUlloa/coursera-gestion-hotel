{
  "name": "guia-hoteles",
  "version": "1.0.0",
  "description": "Nuestro primer proyecto de node js",
  "main": "index.html",
  "scripts": {
    "dev": "lite-server",
    "start": "concurrently \"npm run scss\"  \" npm run dev\"  ",
    "test": "echo \"Error: no test specified\" && exit 1",
    "scss": "node-sass -o  css/ css/ ",
    "less": "lessc css/styles2.less css/styles2.css ",
    "watch:scss": "onchange 'css/*.scss' -- npm run scss ",
    "clean": "rimraf dist",
    "imagemin": "imagemin img/* --out-dir dist/img  ",
    "usemin": "usemin index.html -d dist -o dist/index.html  &&  usemin about.html -d dist -o dist/about.html  &&  usemin about_2.html -d dist -o dist/about_2.html &&  usemin about_3.html -d dist -o dist/about_3.html ",
    "build": "npm run clean &&  npm run scss &&  npm run imagemin && npm run usemin "
  },
  "author": "Kenneth Ulloa",
  "license": "ISC",
  "devDependencies": {
    "copyfiles": "^2.4.1",
    "grunt": "^1.6.1",
    "grunt-browser-sync": "^2.2.0",
    "grunt-contrib-clean": "^2.0.1",
    "grunt-contrib-concat": "^2.1.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-cssmin": "^4.0.0",
    "grunt-contrib-imagemin": "^4.0.0",
    "grunt-contrib-sass": "^2.0.0",
    "grunt-contrib-uglify": "^5.2.2",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-filerev": "^2.3.1",
    "grunt-usemin": "^3.1.1",
    "gulp": "^4.0.2",
    "gulp-sass": "^5.1.0",
    "jit-grunt": "^0.10.0",
    "lite-server": "^2.6.1",
    "node-less": "^1.0.0",
    "node-sass": "^8.0.0",
    "onchange": "^7.1.0",
    "rimraf": "^4.4.0",
    "time-grunt": "^2.0.0"
  },
  "dependencies": {
    "bootstrap": "^5.2.3",
    "bootstrap-icons": "^1.10.3",
    "concurrently": "^7.6.0",
    "cssmin": "^0.4.3",
    "htmlmin": "^0.0.7",
    "install": "^0.13.0",
    "jquery": "^3.6.3",
    "less": "^4.1.3",
    "npm": "^9.6.1",
    "open-icons": "^1.0.5",
    "popper.js": "^1.16.1",
    "save": "^2.9.0",
    "uglifyjs": "^2.4.11",
    "usemin-cli": "^0.6.0"
  }
}
